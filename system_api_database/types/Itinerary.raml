#%RAML 1.0 DataType
type: object
properties:
  id:
    type: integer
  user_id:
    type: integer
  name:
    type: string
  start_date:
    type: date-only
  end_date:
    type: date-only
  locations:
    type: array
    items:
      type: object
      properties:
        location_id:
          type: integer
        position:
          type: integer
  created_at:
    type: datetime